{
	"compilerOptions": {
		"strictNullChecks": true,
		"allowJs": true,
		"esModuleInterop": false,
		"jsx": "react-native",
		"lib": ["DOM", "ESNext"],
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"module": "ESNext",
		"noEmit": true,
		"noImplicitAny": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"target": "ESNext"
	},
	"include": [
		"App.tsx",
		"src/*",
		"src/Setup/date.ts",
		"src/Setup/constants.ts"
, "src/Setup/timers.ts", "src/Setup/sentry.ts", "src/Setup/notifications.ts"	],
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	],
	"extends": "expo/tsconfig.base"
}


