{
	"cli": {
		"version": ">= 7.0.0"
	},
	"build": {
		"development": {
			"channel": "development",
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"DEV": "true"
			}
		},
		"preview": {
			"channel": "preview",
			"distribution": "internal",
			"android": {
				"buildType": "apk",
				"gradleCommand": ":app:assembleRelease -PreactNativeArchitectures=armeabi-v7a,arm64-v8a -x lint -x lintVitalRelease -x lint"
			}
		},
		"production-ios": {
			"channel": "production",
			"ios": {},
			"distribution": "internal"
		},
		"production": {
			"channel": "production",
			"android": {
				"buildType": "apk",
				"gradleCommand": ":app:assembleRelease -PreactNativeArchitectures=armeabi-v7a,arm64-v8a -x lint -x lintVitalRelease -x lint"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
